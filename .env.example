# 通用配置
# PostgreSQL 连接串（docker-compose 下无需改动）
DATABASE_URL=postgresql://user:password@localhost:5432/wellesley

# 请求与重试
TIMEOUT_SECONDS=15
RETRY_TOTAL=3
RETRY_BACKOFF=0.5

# 随机延时（降低服务端压力，单位：秒）
# 建议设置一个范围，例如最小0.4，最大1.2
DELAY_MIN_SECONDS=0.4
DELAY_MAX_SECONDS=1.2

# 网页登录（可选，不配置则免登录）
WEB_USERNAME=admin
WEB_PASSWORD=admin123
# Flask会话密钥（请修改为随机字符串）
SECRET_KEY=please-change-me

# 平台展示名称（可选，用于网页显示）
TIGA_DISPLAY_NAME=Tiga
GAIA_DISPLAY_NAME=Gaia

# ============ Tiga 平台配置 ============
# 目标服务主机地址（必须）
TIGA_BASE_URL=https://app.example.com
TIGA_USER_AGENT=
TIGA_ACCEPT_LANGUAGE="zh-Hans-CN;q=1, en-CN;q=0.9"

# Tiga 平台请求参数
TIGA_CITY_ID=25
TIGA_DEVICE=iPhone15,2
TIGA_DEVICE_UU_TOKEN=
TIGA_CHANNEL=appstore
TIGA_PLATFORM=1
TIGA_SYS_VERSION=ios18.6
TIGA_REGISTRATION_ID=
TIGA_TOKEN=

# Tiga 调度与分类
TIGA_SCHEDULE_INTERVAL_MINUTES=30
TIGA_DOMESTIC_CATEGORY_ID=232
TIGA_OVERSEAS_CATEGORY_ID=836
# 可选：限制每轮最大页数
# TIGA_MAX_PAGES=10

# ============ Gaia 平台配置 ============
# 目标服务主机地址
GAIA_BASE_URL=https://route-api.example.com
GAIA_USER_AGENT="Mozilla/5.0 (iPhone; CPU iPhone OS 18_6 like Mac OS X) AppleWebKit/605.1.15 (KHTML, like Gecko) Mobile/15E148 MicroMessenger/8.0.62(0x18003e3a) NetType/4G Language/zh_CN"
GAIA_ACCEPT_LANGUAGE=zh-CN

# Gaia 调度与分类
GAIA_SCHEDULE_INTERVAL_MINUTES=60
GAIA_CATALOGS=E,L,SW,S,WE,SY
# 可选：限制每轮最大页数
# GAIA_MAX_PAGES=5
