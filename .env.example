# 目标服务主机地址（必须）
BASE_URL=https://app.example.com

# 请求与重试
TIMEOUT_SECONDS=15
RETRY_TOTAL=3
RETRY_BACKOFF=0.5

# 随机延时（降低服务端压力，单位：秒）
# 建议设置一个范围，例如最小0.4，最大1.2
DELAY_MIN_SECONDS=0.4
DELAY_MAX_SECONDS=1.2

# 公共请求参数（可按需填写）
CITY_ID=25
DEVICE=iPhone15,2
DEVICE_UU_TOKEN=
CHANNEL=appstore
PLATFORM=1
SYS_VERSION=ios18.6
USER_AGENT=
ACCEPT_LANGUAGE=zh-Hans-CN;q=1, en-CN;q=0.9
REGISTRATION_ID=
TOKEN=

# PostgreSQL 连接串（docker-compose 下无需改动）
DATABASE_URL=postgresql://user:password@localhost:5432/wellesley

# 调度与分类
SCHEDULE_INTERVAL_MINUTES=30
DOMESTIC_CATEGORY_ID=232
OVERSEAS_CATEGORY_ID=836
# 可选：限制每轮最大页数
# MAX_PAGES=10

# 网页登录（可选，不配置则免登录）
WEB_USERNAME=admin
WEB_PASSWORD=admin123
# Flask会话密钥（请修改为随机字符串）
SECRET_KEY=please-change-me
